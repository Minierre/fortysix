{
  "name": "boilermaker",
  "version": "1.0.0",
  "description": "Some boilerplate code to get you started - get shakin'!",
  "engines": {
    "node": ">= 7.0.0"
  },
  "main": "index.js",
  "scripts": {
    "build-client": "webpack",
    "build-client-watch": "webpack -w",
    "deploy": "script/deploy",
    "lint": "eslint ./ --ignore-path .gitignore",
    "postinstall": "touch secrets.js",
    "seed": "node script/seed.js",
    "start": "node server",
    "start-dev": "NODE_ENV='development' npm run build-client-watch & VESNA_IS_AWESOME='true' npm run start-server",
    "start-server": "nodemon server -e html,js,scss --ignore public",
    "test": "NODE_ENV='test' DATABASE_URL='postgres://localhost:5432/partonia' mocha ./server/**/*.spec.js ./server/**/**/*.spec.js ./client/**/*.spec.js --compilers js:babel-register",
    "test-server-watch": "NODE_ENV='test' DATABASE_URL='postgres://localhost:5432/partonia' mocha ./server/**/*.spec.js -w",
    "test-utils-watch": "NODE_ENV='test' DATABASE_URL='postgres://localhost:5432/partonia' mocha ./server/utils/**/*.spec.js -w"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.15.3",
    "big.js": "^5.0.3",
    "body-parser": "^1.16.1",
    "chai-http": "^3.0.0",
    "chalk": "^2.3.0",
    "codemirror": "^5.33.0",
    "compression": "^1.7.1",
    "connect-session-sequelize": "^4.1.0",
    "express": "^4.14.1",
    "express-session": "^1.15.1",
    "formik": "^0.10.5",
    "history": "^4.6.3",
    "js-algorithms": "^1.4.1",
    "konva": "^1.7.6",
    "material-ui": "^0.20.0",
    "moment": "^2.20.1",
    "passport": "^0.3.2",
    "passport-google-oauth": "^1.0.0",
    "pg": "^6.1.2",
    "pg-hstore": "^2.3.2",
    "prop-types": "^15.5.8",
    "react": "^16.0.0",
    "react-bootstrap": "^0.32.0",
    "react-bootstrap-table": "^4.3.1",
    "react-codemirror2": "^3.0.7",
    "react-dom": "^16.0.0",
    "react-json-view": "^1.16.0",
    "react-konva": "^1.6.6",
    "react-loading": "^1.0.3",
    "react-redux": "^5.0.2",
    "react-router-dom": "^4.1.1",
    "recharts": "^1.0.0-beta.10",
    "redux": "^3.6.0",
    "redux-freeze": "^0.1.5",
    "redux-logger": "^2.8.1",
    "redux-thunk": "^2.2.0",
    "sandbox": "^0.8.6",
    "sequelize": "^4.3.1",
    "socket.io": "^2.0.3",
    "threads": "^0.10.1",
    "uuid": "^3.2.1",
    "volleyball": "^1.4.1"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.9.0",
    "babel-core": "^6.22.1",
    "babel-eslint": "^8.1.2",
    "babel-loader": "^6.2.10",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^3.5.0",
    "css-loader": "^0.28.8",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "^1.0.0",
    "eslint": "^4.10.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.4.0",
    "ignore-styles": "^5.0.1",
    "mocha": "^3.5.3",
    "nodemon": "^1.12.1",
    "react-test-renderer": "^16.0.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-mock-store": "^1.2.3",
    "style-loader": "^0.19.1",
    "supertest": "^3.0.0",
    "webpack": "^2.2.1",
    "webpack-livereload-plugin": "^1.0.0"
  }
}
